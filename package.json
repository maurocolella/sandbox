{
  "name": "whitepaper-workspace",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "version": "0.0.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "workspaces": [
    "mol-viewer",
    "mol-renderer",
    "pdb-parser"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "build:viewer": "pnpm --filter mol-viewer build",
    "build:renderer": "pnpm --filter mol-renderer build",
    "build:parser": "pnpm --filter pdb-parser build",
    "dev:viewer": "pnpm -C mol-viewer dev",
    "dev:renderer": "pnpm -C mol-renderer dev",
    "test": "pnpm -r test",
    "clean": "pnpm -r clean || true"
  },
  "devDependencies": {
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "tailwindcss": "^4.1.14"
  }
}
